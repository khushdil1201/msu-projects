  object ADOQuery4: TADOQuery
    Active = True
    Connection = Form6.ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      ''
      
        'SELECT Course,BirthDay,Speciality,id,CONCAT(LastName,'#39' '#39', FirstN' +
        'ame,'#39' '#39', FatherName) AS FIO FROM students WHERE CONCAT(LastName,' +
        #39' '#39', FirstName,'#39' '#39', FatherName) = '#39#1057#1086#1083#1080#1077#1074' '#1064#1091#1093#1088#1072#1090' '#1064#1072#1088#1072#1092#1079#1086#1076#1072#39';')
    Left = 864
    Top = 379
  end
